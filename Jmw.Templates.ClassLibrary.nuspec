<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>Jmw.Templates.ClassLibrary</id>
    <version>0.1.0</version>
    <description>
      C# class library template with StyleCop and GitVersionTask.
    </description>
    <projectUrl>https://github.com/jmw-fr/Jmw.Templates.ClassLibrary</projectUrl>
    <authors>Jean-Marc Weeger</authors>
    <license type="expression">MIT</license>
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
  </metadata>
  <files>
    <file src="ClassLibrary.sln" target="content" />
    <file src="readme.md" target="content" />
    <file src="stylecop.*" target="content" />
    <file src="src\**" target="content" exclude="**\bin\**;**\obj\**" />
    <file src="test\**" target="content" exclude="**\bin\**;**\obj\**" />
  </files>
</package>